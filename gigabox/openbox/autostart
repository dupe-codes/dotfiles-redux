# Load set nitrogen desktop wallpaper
nitrogen --restore &

# Start tint2 panel
tint2 &

# Start picom compositor
picom -b

# Start dunst notification daemon
dunst &

# Start power manager
xfce4-power-manager &

# Volume control
# pavucontrol &

# Network management
nm-applet &

# 1password tray
1password --silent &

